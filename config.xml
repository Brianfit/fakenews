<?xml version='1.0' encoding='utf-8'?>
<widget id="com.dopeykleptocrat.onionapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Fake News: A Drinking Game</name>
    <description>
        A True or False game that serves up real headlines and satirical ones. Guess wrong, you drink. 
    </description>
    <author email="brianfit58@gmail.com" href="http://dopeykleptocrat.com">
        Team Dopey Kelptocrat
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="https://google.com/*" />
    <access origin="http://google.com" />
    <access origin="https://google.com" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-admob" spec="~4.1.16" />
    <plugin name="cordova-plugin-device" spec="^2.0.2" />
    <engine name="ios" spec="^4.5.5" />
</widget>
